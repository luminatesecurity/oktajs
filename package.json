{
  "name": "oktajs",
  "description": "An interface for the Okta REST API from nodejs",
  "author": "Jon Davis <jon@snowulf.com>",
  "contributors": [
    "Josh Johnson <jjohnson@okta.com>",
    "Kevin He <kevin.he@okta.com>"
  ],
  "tags": [
    "okta",
    "identity",
    "sso",
    "security"
  ],
  "dependencies": {
    "request": "2.75.x",
    "should": "^11.2.1"
  },
  "devDependencies": {
    "chai": "3.5.x",
    "jshint": "2.9.x",
    "mocha": "3.1.x"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/snowulf/oktajs.git"
  },
  "bugs": {
    "url": "http://github.com/snowulf/oktajs/issues"
  },
  "main": "index.js",
  "version": "3.0.2",
  "engines": "node >= 0.10.40",
  "license": "MIT",
  "scripts": {
    "alltest": "npm run lint && mocha --timeout 3000 --slow 1000 tests/main.js",
    "test": "mocha --timeout 3000 --slow 1000 tests/main.js",
    "lint": "jshint lib/*.js"
  },
  "jshintConfig": {
    "boss": true,
    "node": true,
    "strict": true,
    "newcap": false,
    "undef": true,
    "unused": true,
    "onecase": true,
    "lastsemic": true,
    "indent": 2,
    "globals": {
      "escape": true,
      "describe": true,
      "it": true,
      "before": true
    }
  }
}
